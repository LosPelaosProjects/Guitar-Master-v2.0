package proyectocatalogodenotas;

import javax.swing.*;
import java.applet.AudioClip;

public class InterfazNotas extends javax.swing.JFrame {

    int op = 0;
    ImageIcon Imagen[] = new ImageIcon[28];
    ImageIcon Notas[] = new ImageIcon[25];

    public InterfazNotas() {
        initComponents();
        this.setTitle("Guitar Master");
        for (int i = 0; i < Imagen.length; i++) {
            Imagen[0] = new ImageIcon(getClass().getResource("/proyectocatalogodenotas/imagenes/GM.png"));
            Notas[0] = new ImageIcon(getClass().getResource("/proyectocatalogodenotas/notaseleccionada/solo.png"));

        }
        jLabel1.setIcon(Imagen[0]);
        jLabel6.setIcon(Notas[0]);
    }

   private void imagenNota(int op, String ruta1, String ruta2) {

        Imagen[op] = new ImageIcon(getClass().getResource(ruta1));
        jLabel1.setIcon(Imagen[op]);
        Notas[op] = new ImageIcon(getClass().getResource(ruta2));
        jLabel6.setIcon(Notas[op]);
    }

        private void audioNota(String ruta) { // Método de audio, que recibe la ruta (dirrecion) donde se encuentra el audio.
        AudioClip sonido;
        sonido = java.applet.Applet.newAudioClip(getClass().getResource(ruta));
        sonido.play();
        
    }

